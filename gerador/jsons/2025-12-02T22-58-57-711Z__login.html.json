    "arquivo": "login.html",
    "conteudo": "<!DOCTYPE html>\n<html lang=\"pt-BR\">\n<head>\n  <meta charset=\"UTF-8\" />\n  <title>Login • PBQE-C V2</title>\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n  <link rel=\"stylesheet\" href=\"style.css\" />\n  <link rel=\"stylesheet\" href=\"usuarios.css\" />\n</head>\n<body class=\"page page-auth\">\n  <div class=\"auth-wrapper\">\n    <div class=\"auth-card\">\n      <header class=\"auth-header\">\n        <h1 class=\"auth-title\">Entrar</h1>\n        <p class=\"auth-subtitle\">Faça login para acessar o Sistema Central</p>\n      </header>\n\n      <form id=\"formLogin\" class=\"auth-form\">\n        <div class=\"form-group\">\n          <label for=\"loginUsuario\">Usuário</label>\n          <input id=\"loginUsuario\" name=\"usuario\" type=\"text\" autocomplete=\"username\" required />\n        </div>\n\n        <div class=\"form-group\">\n          <label for=\"loginSenha\">Senha</label>\n          <input id=\"loginSenha\" name=\"senha\" type=\"password\" autocomplete=\"current-password\" required />\n        </div>\n\n        <button type=\"submit\" class=\"btn btn-primary btn-full\">Entrar</button>\n      </form>\n\n      <div id=\"msgLogin\" class=\"auth-message\" aria-live=\"polite\"></div>\n\n      <section class=\"auth-extra\">\n        <button id=\"btnIrConfirmar\" class=\"btn btn-text\">Confirmar e-mail</button>\n        <button id=\"btnIrCadastro\" class=\"btn btn-text\">Criar nova conta</button>\n      </section>\n    </div>\n  </div>\n\n  <script src=\"usuarios.js\"></script>\n</body>\n</html>\n"
