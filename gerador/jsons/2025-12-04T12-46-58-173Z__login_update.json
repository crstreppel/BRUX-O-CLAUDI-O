    "conteudo": "<!-- PBQE-C update 2025-12-04 -->\n<!DOCTYPE html>\n<html lang=\"pt-BR\">\n<head>\n  <meta charset=\"UTF-8\" />\n  <title>Login • PBQE-C V2</title>\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n  <link rel=\"stylesheet\" href=\"style.css\" />\n  <link rel=\"stylesheet\" href=\"usuarios.css\" />\n</head>\n<body class=\"page page-auth\">\n  <div class=\"auth-wrapper\">\n    <div class=\"auth-card\">\n      <header class=\"auth-header\">\n        <h1 class=\"auth-title\">Entrar</h1>\n        <p class=\"auth-subtitle\">Faça login para acessar o Sistema Central</p>\n      </header>\n      <form id=\"formLogin\" class=\"auth-form\">\n        <div class=\"form-group\">\n          <label for=\"loginEmail\">E-mail</label>\n          <input id=\"loginEmail\" name=\"email\" type=\"email\" autocomplete=\"email\" required />\n        </div>\n        <div class=\"form-group\">\n          <label for=\"loginSenha\">Senha</label>\n          <input id=\"loginSenha\" name=\"senha\" type=\"password\" autocomplete=\"current-password\" required />\n        </div>\n        <button type=\"submit\" class=\"btn btn-primary btn-full\">Entrar</button>\n      </form>\n      <div id=\"msgLogin\" class=\"auth-message\" aria-live=\"polite\"></div>\n      <section class=\"auth-extra\">\n        <button id=\"btnIrConfirmar\" class=\"btn btn-text\">Confirmar e-mail</button>\n        <button id=\"btnIrCadastro\" class=\"btn btn-text\">Criar nova conta</button>\n      </section>\n    </div>\n  </div>\n  <script src=\"usuarios.js\"></script>\n</body>\n</html>\n"
