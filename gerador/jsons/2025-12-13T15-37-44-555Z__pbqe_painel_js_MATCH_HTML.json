  "caminho": "../backend/public/js",
  "arquivo": "painel.js",
  "conteudo": "document.addEventListener('DOMContentLoaded',()=>{const u=sessionStorage.getItem('usuarioAtual')||localStorage.getItem('usuarioAtual');if(!u){window.location.href='login.html';return}const e=document.getElementById('dashboardUser');e&&(e.textContent=u);const l=document.getElementById('logoutBtn');l&&l.addEventListener('click',()=>{sessionStorage.removeItem('usuarioAtual');localStorage.removeItem('usuarioAtual');localStorage.removeItem('token');localStorage.removeItem('usuario');window.location.href='login.html'})});"
}